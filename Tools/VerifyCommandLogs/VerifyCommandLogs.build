<?xml version="1.0"?>
<project name="VerifyCommandLogs" default="debug">
    <target name="debug" depends="init">
        <csc output="bin/VerifyCommandLogs.exe" target="exe" debug="True">
            <sources basedir="src">
                <include name="*.cs" />
            </sources>
        </csc>
    </target>
    <target name="init">
        <mkdir dir="bin" />
    </target>
</project>